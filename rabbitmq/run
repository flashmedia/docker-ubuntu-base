
# test if the container is already running
EXISTS="$(docker ps -a | grep -c 'mms-message-queue[^-:]')"

# does the container exist?
if [ $EXISTS = "1" ]; then
    # remove it so that the command below doesn't error (discard output)
    docker rm -f mms-message-queue > /dev/null 2>&1
fi

docker run -d -p 5672:5672 -p 15672:15672 flashmedia/mms-message-queue
